    date: ""
    hour: "07:00"
    activity: "Badminton"
  - day: "Thu"
    date: ""
    hour: "10:00"
    activity: "Badminton"
  - day: "Fri"
    date: ""
    hour: "14:00"
    activity: "Tennis"

